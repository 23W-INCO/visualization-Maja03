{
  "name": "My Codespace",
  "dockerComposeFile": "docker-compose.yml",
  "service": "app",
  "workspaceMount": "source=.",
  "runArgs": ["-u", "codespace"],
  "settings": {
    "terminal.integrated.shell.linux": "/bin/bash"
  },
  "extensions": ["ms-python.python"],
  "postCreateCommand": "python app.py"  
}
